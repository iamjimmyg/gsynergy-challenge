{
  "name": "gsynergy-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.2",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dom": "^16.4.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar public/css/ -o public/css/",
    "watch-css": "npm run build-css && node-sass-chokidar public/css/ -o public/css --watch --recursive",
